#include <stdio.h>
#include <math.h>
int main(int argc, char* argv[])
{
    double a, b, div, rad;
    do
    {
        printf("Inserisci valore di a");
        scanf("%lf", &a);
        printf("Inserisci valore di b");
        scanf("%lf", &b);
        if (a < b)
            div = b / a;
        else
            div = b / a;
        if (div > 0)
        {
            rad = sqrt(div);
            printf("La radice vale %f\n\n", rad);
        }
    }
    while (div > 0);
    printf("Non Ã© possibile fare la radice di un numero negativo\n\n");

    return 0;
}
